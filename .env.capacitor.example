
# API Server URL - Change this to your deployed API server
NEXT_PUBLIC_API_URL=https://your-api-server.com
NEXT_PUBLIC_APP_URL=https://your-api-server.com

# Or for local development (replace with your computer's IP)
# NEXT_PUBLIC_API_URL=http://192.168.1.100:3000
# NEXT_PUBLIC_APP_URL=http://192.168.1.100:3000

# Database (use the same as your deployed server)
DATABASE_URL="your-database-url"

# NextAuth (use the same as your deployed server)
NEXTAUTH_SECRET="your-nextauth-secret"
NEXTAUTH_URL=https://your-api-server.com
